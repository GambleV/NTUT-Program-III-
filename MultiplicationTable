<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>103AC2024 MultiplicationTable</title>
	<script type="text/javascript">
		MultiplicationTable(6,1);

		function MultiplicationTable(i,j){
			if(i<10){
				if(j<10){
					//console.log(i*j);
					alert((i*j)%6==0 ? '*$' : 
								((i*j)%2==0 ? '$' : 
								((i*j)%3==0 ? '*' : (i*j))));
					MultiplicationTable(i,j+1);
				}
				else{
					i++;
					MultiplicationTable(i,1);
				}
			}
		}
	</script>
</head>
<body>
	
</body>
</html>